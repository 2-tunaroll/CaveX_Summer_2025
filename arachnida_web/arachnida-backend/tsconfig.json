{
  "compilerOptions": {
    "target": "es2016",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    // "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
	"moduleResolution": "node",
	"outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "ts-node": {
	  "esm": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
